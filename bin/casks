#!/bin/bash

brew install caskroom/cask/brew-cask
brew cask update
brew cask upgrade

for cask in \
    1password \
    dash /
    docker /
    dropbox /
    firefox /
    flux /
    insomnia /
    google-chrome /
    little-snitch /
    spectacle /
    vagrant /
    vagrant-manager /
    virtualbox;
do
    brew cask install $cask
done
